package com.spring;

public class singletonClassDemo {
public static void main(String[] args) {
	SingletonEmployee e=new SingletonEmployee.getst();
	SingletonEmployee e1=new SingletonEmployee.getst();
	SingletonEmployee e2=new SingletonEmployee.getst();
	//if(e!=null)
	e.display();
	//if(e1!=null)
	e1.display();
	
	
	//if(e2!=null)
		e2.display();
		System.out.println(e);

	System.out.println(e1);

	System.out.println(e2);
}
}
