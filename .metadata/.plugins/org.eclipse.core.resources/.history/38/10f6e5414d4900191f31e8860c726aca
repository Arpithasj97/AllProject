package legato;

public class NoRepeatAlpha {
 public static void main(String[] args) {
	 String s="wElcome to legato";
		 for (int i = 0; i <=s.length()-1; i++) {
	    	 int count = 0 ;
	        for (int j = 0; j < s.length(); j++) {
	        	
	            if(s.charAt(i)==s.charAt(j))
	            {
	                count++;

	            }
	        }   

	 }
 }
}
