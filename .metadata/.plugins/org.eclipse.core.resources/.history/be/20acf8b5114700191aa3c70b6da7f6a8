package com.spring;

public class singletonClassDemo {
public static void main(String[] args) {
	singletonEmployee e=new singletonEmployee("IIHT");
	singletonEmployee e1=new singletonEmployee("LEGATO");
	singletonEmployee e2=new singletonEmployee("Abhi");
	if(e!=null)
	e.display();
	if(e1!=null)
	e1.display();
	
	
	if(e2!=null)
		e2.display();
		System.out.println(e);

	System.out.println(e1);

	System.out.println(e2);
}
}
